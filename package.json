{
  "name": "dicom-dimse-native",
  "version": "1.3.2",
  "description": "native addon using DCMTK dicom toolkit",
  "main": "index.js",
  "scripts": {
    "install": "npx prebuild-install -r napi || npx cmake-js compile",
    "prebuild": "npx prebuild -t 4 -r napi --all --verbose --backend cmake-js",
    "build": "tsc",
    "example:echoscu": "ts-node ./examples/echoscu.ts",
    "example:findscu": "ts-node ./examples/findscu.ts",
    "example:getscu": "node ./examples/getscu.js",
    "example:movescu": "node ./examples/movescu.js",
    "example:storescu": "node ./examples/storescu.js",
    "example:storescp": "node ./examples/storescp.js",
    "example:shutdownscu": "node ./examples/shutdownscu.js",
    "test": "echo \"no test\" && exit 0"
  },
  "keywords": [
    "DICOM",
    "DIMSE",
    "NATIVE",
    "DICOM-DIMSE",
    "DICOM-DIMSE-NATIVE",
    "DCMTK",
    "NETWORK",
    "DICOMJS",
    "SCU",
    "SCP",
    "C-ECHO",
    "C-FIND",
    "C-MOVE",
    "C-STORE",
    "PACS"
  ],
  "author": "Michael Knopke",
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.5.0",
    "cmake-js": "^6.2.1",
    "node-addon-api": "^4.2.0",
    "prebuild-install": "^6.1.4"
  },
  "engines": {
    "node": ">=10"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/knopkem/dicom-dimse-native"
  },
  "binary": {
    "napi_versions": [
      4
    ]
  },
  "devDependencies": {
    "@types/node": "^16.11.3",
    "ts-node": "^10.3.1",
    "tsc": "^2.0.3",
    "typescript": "^4.4.4"
  }
}
